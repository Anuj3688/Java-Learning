<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 	xsi:schemaLocation="
 	http://www.springframework.org/schema/beans 
 	http://www.springframework.org/schema/beans/spring-beans.xsd
 	http://www.springframework.org/schema/context/spring-context.xsd">
 	<!-- bean definitions here -->
 
 	<bean class="com.qspider.Dependancy.Bean.Employee" name="employee">
 		<property name="id" value="1"/>
 		<property name="name" value="Amol"/>
 		<property name="details" ref="empdetails"/>
 		
 	</bean>
 
	<bean class="com.qspider.Dependancy.Bean.EmployeeDetails" name="empdetails"> 
 
 	<property name="email" value="amol@gmail.com"/>
 	<property name="address" value="pune"/>
 	<property name="salary" value="45000"/>
 	
 	
 	</bean>
</beans>