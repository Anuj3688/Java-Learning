<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
	
	<bean class="com.jspider.SpringCoreDependencyAssignment.Main.Main.Bank" name="bank">
		<property name="id" value="101"/>
		<property name="name" value="SBI"/>
		<property name="bank" ref="bankDetails"/>
		
	</bean>
	
	<bean class="com.jspider.SpringCoreDependencyAssignment.Main.Main.BankDetails" name="bankDetails">
		<property name="id" value="121"/>
		<property name="name" value="SBI"/>
		<property name="ifsc" value="sbi001"/>
		<property name="turnOver" value="500"/>
		<property name="totalCustomers" value="200"/>
	</bean>
</beans>