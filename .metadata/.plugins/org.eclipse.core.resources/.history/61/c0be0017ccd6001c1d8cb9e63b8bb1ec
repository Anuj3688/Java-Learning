package com.jspider.SpringCoreDependencyAssignment.Main;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class BankCustomerMain {
	public static void main(String[] args) {
		ApplicationContext ctx = new ClassPathXmlApplicationContext(
				"SpringCoreDependencyAssignment/src/main/java/com/jspider/SpringCoreDependencyAssignment/Main/Bank.xml");
		Bank bean = (Bank) ctx.getBean(Bank.class);
		System.out.println(bean);

		((ClassPathXmlApplicationContext) ctx).close();
	}
}
