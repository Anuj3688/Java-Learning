package com.jspider.SpringCoreUsingConstructor.Main;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class EmployeeMain {
	public static void main(String[] args) {
		ApplicationContext amol = new ClassPathXmlApplicationContext("Students.xml");
		StudentBean bean = (StudentBean) amol.getBean(StudentBean.class);
		System.out.println(bean);

		((ClassPathXmlApplicationContext) amol).close();
	}
}
